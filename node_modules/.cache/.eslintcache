isSubmitting}\n              fullWidth\n              variant=\"contained\"\n              color=\"primary\"\n              type=\"submit\"\n              sx={{ marginRight: 1 }}\n            >\n              Create\n            </Button>\n            <Button component={Link} to=\"/stock\" variant=\"outlined\" fullWidth>\n              Cancl\n            </Button>\n          </CardActions>\n        </Card>\n      </Form>\n    );\n  };\n\n  const initialValues: Product = { name: \"\", stock: 2000, price: 3000 };\n\n  return (\n    <Box>\n      <Formik\n        validate={(values) => {\n          let errors: any = {};\n          if (!values.name) errors.name = \"Enter name\";\n          if (values.stock < 10) errors.stock = \"Min stock is not lower than 10\";\n          if (values.price < 100) errors.price = \"Min price is not lower than 100\";\n          return errors;\n        }}\n        initialValues={initialValues}\n        onSubmit={(values, { setSubmitting }) => {\n          // alert(JSON.stringify(values));\n          let formData = new FormData();\n          formData.append(\"name\", values.name);\n          formData.append(\"price\", String(values.price));\n          formData.append(\"stock\", String(values.stock));\n          formData.append(\"image\", values.file);\n          dispatch(stockActions.addProduct(formData));\n          setSubmitting(false);\n        }}\n      >\n        {(props: any) => showForm(props)}\n      </Formik>\n    </Box>\n  );\n};\n\nexport default StockCreatePage;\n","/Users/chaiyasittayabovorn/Desktop/Training/ReactJS_Training/video_online/cmreactjs_mui_stockworkshop/src/components/pages/StockEditPage/index.ts",[],[],"/Users/chaiyasittayabovorn/Desktop/Training/ReactJS_Training/video_online/cmreactjs_mui_stockworkshop/src/components/pages/StockEditPage/StockEditPage.tsx",["251","252","253","254","255","256"],[],"import { Card, CardContent, Typography, CardActions, Button, Box } from \"@mui/material\";\nimport { FormikProps, Form, Field, Formik } from \"formik\";\nimport { TextField } from \"formik-material-ui\";\nimport * as React from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Link, useMatch, useNavigate } from \"react-router-dom\";\nimport { imageUrl } from \"../../../Constants\";\nimport { RootReducers } from \"../../../reducers\";\nimport { Product } from \"../../../types/product.type\";\nimport * as stockEditActions from \"./../../../actions/stock.edit.action\";\n\ntype StockEditPageProps = {\n  //\n};\n\nconst StockEditPage: React.FC<any> = () => {\n  const dispatch = useDispatch();\n  const stockEditReducer = useSelector((state: RootReducers) => state.stockEditReducer);\n\n  const match = useMatch(\"/stock/edit/:id\");\n  React.useEffect(() => {\n    let id = match?.params.id;\n    dispatch(stockEditActions.getProductById(id));\n  }, []);\n\n  const showPreviewImage = (values: any) => {\n    if (values.file_obj) {\n      return <img src={values.file_obj} style={{ height: 100 }} />;\n    } else if (values.image) {\n      return <img src={`${imageUrl}/images/${values.image}`} style={{ height: 100 }} />;\n    }\n  };\n\n  const showForm = ({ values, setFieldValue, isSubmitting }: FormikProps<Product>) => {\n    return (\n      <Form>\n        <Card>\n          <CardContent sx={{ padding: 4 }}>\n            <Typography gutterBottom variant=\"h3\">\n              Edit Stock\n            </Typography>\n\n            <Field style={{ marginTop: 16 }} fullWidth component={TextField} name=\"name\" type=\"text\" label=\"Name\" />\n            <br />\n            <Field style={{ marginTop: 16 }} fullWidth component={TextField} name=\"price\" type=\"number\" label=\"Price\" />\n\n            <Field style={{ marginTop: 16 }} fullWidth component={TextField} name=\"stock\" type=\"number\" label=\"Stock\" />\n\n            <div style={{ margin: 16 }}>{showPreviewImage(values)}</div>\n\n            <div>\n              <img src={`${process.env.PUBLIC_URL}/images/ic_photo.png`} style={{ width: 25, height: 20 }} />\n              <span style={{ color: \"#00B0CD\", marginLeft: 10 }}>Add Picture</span>\n\n              <input\n                type=\"file\"\n                onChange={(e: React.ChangeEvent<any>) => {\n                  e.preventDefault();\n                  setFieldValue(\"file\", e.target.files[0]); // for upload\n                  setFieldValue(\"file_obj\", URL.createObjectURL(e.target.files[0])); // for preview image\n                }}\n                name=\"image\"\n                click-type=\"type1\"\n                multiple\n                accept=\"image/*\"\n                id=\"files\"\n                style={{ padding: \"20px 0 0 20px\" }}\n              />\n            </div>\n          </CardContent>\n          <CardActions>\n            <Button\n              disabled={isSubmitting}\n              fullWidth\n              variant=\"contained\"\n              color=\"primary\"\n              type=\"submit\"\n              sx={{ marginRight: 1 }}\n            >\n              Edit\n            </Button>\n            <Button component={Link} to=\"/stock\" variant=\"outlined\" fullWidth>\n              Cancl\n            </Button>\n          </CardActions>\n        </Card>\n      </Form>\n    );\n  };\n\n  const initialValues: Product = { name: \"Loading...\", stock: 0, price: 0 };\n\n  return (\n    <Box>\n      <Formik\n        validate={(values) => {\n          let errors: any = {};\n          if (!values.name) errors.name = \"Enter name\";\n          if (values.stock < 10) errors.stock = \"Min stock is not lower than 10\";\n          if (values.price < 100) errors.price = \"Min price is not lower than 100\";\n          return errors;\n        }}\n        enableReinitialize\n        initialValues={stockEditReducer.result ? stockEditReducer.result : initialValues}\n        onSubmit={(values, { setSubmitting }) => {\n          // alert(JSON.stringify(values));\n          let formData = new FormData();\n